{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["cypress"],
    "esModuleInterop": true
  },
  "include": ["cypress.config.ts", "test", "src/**/*"],
  "exclude": ["node_modules", "dist"]
}
